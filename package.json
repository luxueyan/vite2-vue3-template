{
  "name": "operation-frontend2",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "plugin": "tsc -p plugins",
    "mock": "cd mock && ts-node-dev --respawn --transpile-only index.ts",
    "dev:m": "concurrently -k \"yarn:dev\" \"yarn:mock\"",
    "fontmin": "node ./scripts/fontmin.js",
    "sprite": "gulp -f ./scripts/sprite.js sprite",
    "sprite2": "node ./scripts/sprite.js",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,css,scss,md,json}'",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "./scripts/release.sh",
    "commit": "git-cz"
  },
  "dependencies": {
    "ant-design-vue": "2",
    "axios": "^0.19.2",
    "debug": "^4.3.1",
    "lodash-es": "^4.17.15",
    "qs": "^6.9.4",
    "swrv": "^1.0.0-beta.8",
    "vue": "3",
    "vue-router": "4",
    "vuex": "4"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/glob": "^7.1.3",
    "@types/js-yaml": "^3.12.5",
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.4.1",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^14.0.27",
    "@types/qs": "^6.9.4",
    "@vitejs/plugin-vue": "^1.1.5",
    "@vitejs/plugin-vue-jsx": "^1.1.2",
    "@vue/compiler-sfc": "^3.0.5",
    "autoprefixer": "^9.8.5",
    "commitizen": "^4.1.2",
    "concurrently": "^5.3.0",
    "conventional-changelog": "^3.1.21",
    "conventional-changelog-cli": "^2.0.34",
    "cross-env": "^7.0.2",
    "cz-customizable": "^6.3.0",
    "fontmin": "^0.9.8",
    "gulp": "^4.0.2",
    "gulp-csso": "^4.0.1",
    "gulp-imagemin": "^7.1.0",
    "gulp-rename": "^2.0.0",
    "gulp.spritesmith": "^6.11.0",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-proxies-router": "^1.0.0",
    "koa-router": "^9.1.0",
    "lint-staged": "^10.2.11",
    "merge-stream": "^2.0.0",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.3",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "standard-version": "^8.0.2",
    "ts-node-dev": "^1.0.0-pre.56",
    "typescript": "^4.2.3",
    "vinyl-buffer": "^1.0.1",
    "vite": "^2.1.3",
    "vite-plugin-html": "^2.0.3",
    "vite-plugin-svg": "^0.7.0",
    "vite-plugin-yaml": "^1.0.4",
    "vite-server-plugin-proxy": "^1.0.0-rc.1",
    "vite-transform-globby-import": "1.0.0-rc.7",
    "vite-tsconfig-paths": "^2.5.0",
    "vue-tsc": "^0.0.15",
    "vue-tsx-support": "3",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,vue,css,scss,md,json}": [
      "prettier --write",
      "yarn lint",
      "git add"
    ]
  }
}
